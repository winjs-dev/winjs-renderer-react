{
  "name": "@winner-fed/renderer-react",
  "description": "React renderer for WinJS framework",
  "keywords": [
    "winjs",
    "react",
    "renderer",
    "react-renderer",
    "winner-fed",
    "framework",
    "routing",
    "react-router",
    "ssr",
    "typescript"
  ],
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winjs-dev/winjs-renderer-react.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "playwright test",
    "bump": "bumpp"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@playwright/test": "^1.53.2",
    "@rsbuild/core": "^1.4.2",
    "@rslib/core": "^0.17.0",
    "@types/node": "^22.15.34",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@winner-fed/winjs": "*",
    "bumpp": "10.3.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/runtime": "7.28.4",
    "history": "5.3.0",
    "react-router": "7.9.6"
  },
  "peerDevDependencies": {
    "@winner-fed/winjs": "*"
  },
  "peerDependencies": {
    "@winner-fed/utils": "*",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    },
    "@winner-fed/utils": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
